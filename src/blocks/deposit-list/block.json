{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "disciplinary-homes/deposit-list",
	"title": "Deposit List Demo",
	"version": "0.1.0",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example recent/popular deposits block with React frontend.",
	"supports": {
		"html": false
	},
	"textdomain": "disciplinary-homes",
	"editorScript": "file:./index.js",
	"viewScript": "file:./front.js",
	"style": "file:./style-index.css",
	"attributes": {
		"queryMode": {
			"enum": [ "recent", "popular" ],
			"default": "recent"
		},
		"maxDeposits": {
			"type": "number",
			"default": 3
		}
	}
}
