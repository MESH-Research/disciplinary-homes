{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "disciplinary-homes/metrics-row",
	"title": "Metrics Row Demo",
	"version": "0.1.0",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example metrics row block with React frontend.",
	"supports": {
		"html": false
	},
	"textdomain": "disciplinary-homes",
	"editorScript": "file:./index.js",
	"viewScript": "file:./front.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"showFollowerCount": {
			"type": "boolean",
			"default": true
		},
		"showDepositCount": {
			"type": "boolean",
			"default": true
		},
		"showDownloadCount": {
			"type": "boolean",
			"default": true
		}
	}
}
